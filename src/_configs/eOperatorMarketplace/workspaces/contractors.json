    "contextSchema": {
      "contractorMarketplaceRequest": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "label": "ID zlecenia",
            "fieldType": "text",
            "readOnly": true,
            "required": false,
            "aiHint": "Request identifier"
          },
          "workScope": {
            "type": "string",
            "label": "Zakres prac",
            "fieldType": "textarea",
            "readOnly": true,
            "required": false,
            "aiHint": "Work scope description"
          },
          "postalCode": {
            "type": "string",
            "label": "Kod pocztowy",
            "fieldType": "text",
            "readOnly": true,
            "required": false,
            "aiHint": "Location postal code"
          },
          "city": {
            "type": "string",
            "label": "Miejscowo",
            "fieldType": "text",
            "readOnly": true,
            "required": false,
            "aiHint": "City name"
          },
          "heatSource": {
            "type": "array",
            "label": "殴r贸do ciepa",
            "fieldType": "multiselect",
            "readOnly": true,
            "items": {
              "type": "string",
              "enum": ["heat_pump", "pellet_boiler", "wood_gasification"]
            },
            "enumLabels": {
              "heat_pump": "Pompa ciepa",
              "pellet_boiler": "Piec na pellet",
              "wood_gasification": "Piec zgazowujcy drewno"
            },
            "required": false,
            "aiHint": "Required heat sources"
          },
          "thermomodernization": {
            "type": "object",
            "label": "Termomodernizacja",
            "fieldType": "group",
            "readOnly": true,
            "properties": {
              "windows": {
                "type": "number",
                "label": "Okna (ilo)",
                "fieldType": "number",
                "readOnly": true
              },
              "doors": {
                "type": "number",
                "label": "Drzwi (ilo)",
                "fieldType": "number",
                "readOnly": true
              },
              "wallInsulation": {
                "type": "number",
                "label": "Docieplenie cian (m虏)",
                "fieldType": "number",
                "readOnly": true
              },
              "atticInsulation": {
                "type": "number",
                "label": "Docieplenie poddasza (m虏)",
                "fieldType": "number",
                "readOnly": true
              }
            },
            "required": false,
            "aiHint": "Thermomodernization details"
          },
          "publishedDate": {
            "type": "string",
            "label": "Data publikacji",
            "fieldType": "date",
            "readOnly": true,
            "required": false,
            "aiHint": "Publication date"
          },
          "offersCount": {
            "type": "number",
            "label": "Liczba ofert",
            "fieldType": "number",
            "readOnly": true,
            "required": false,
            "aiHint": "Number of offers"
          },
          "averageOfferPrice": {
            "type": "number",
            "label": "rednia cena ofert (PLN)",
            "fieldType": "number",
            "readOnly": true,
            "required": false,
            "aiHint": "Average offer price"
          }
        }
      },
      "contractorProfile": {
        "type": "object",
        "properties": {
          "companyName": {
            "type": "string",
            "label": "Nazwa firmy",
            "fieldType": "text",
            "readOnly": true,
            "required": false,
            "aiHint": "Company name"
          },
          "description": {
            "type": "string",
            "label": "Opis dziaalnoci",
            "fieldType": "textarea",
            "readOnly": true,
            "required": false,
            "aiHint": "Company description"
          },
          "services": {
            "type": "array",
            "label": "Oferowane usugi",
            "fieldType": "multiselect",
            "readOnly": true,
            "required": false,
            "aiHint": "Offered services"
          },
          "experience": {
            "type": "number",
            "label": "Lata dowiadczenia",
            "fieldType": "number",
            "readOnly": true,
            "required": false,
            "aiHint": "Years of experience"
          },
          "serviceArea": {
            "type": "string",
            "label": "Obszar dziaania",
            "fieldType": "text",
            "readOnly": true,
            "required": false,
            "aiHint": "Service area"
          },
          "rating": {
            "type": "number",
            "label": "Ocena",
            "fieldType": "number",
            "readOnly": true,
            "required": false,
            "aiHint": "Average rating"
          },
          "completedProjects": {
            "type": "number",
            "label": "Zrealizowane projekty",
            "fieldType": "number",
            "readOnly": true,
            "required": false,
            "aiHint": "Number of completed projects"
          }
        }
      }
    }
